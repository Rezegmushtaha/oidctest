{
  "group": "Session Management",
  "usage": {
    "session": true,
    "return_type": [
      "C",
      "CI",
      "CT",
      "CIT"
    ]
  },
  "desc": "Uses RP initiated logout to end a Session at the OP. The RP uses session management to register the session state change",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "grant_types": [
            "authorization_code"
          ]
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "scope": [
            "openid",
            "offline_access"
          ],
          "prompt": "consent"
        }
      }
    },
    {
      "AccessToken": {
        "conditional_execution":{
          "return_type": ["CIT","CI","C","CT"]
        }
      }
    },
    "SessionCheck",
    {
      "EndSession": {
        "set_post_logout_redirect_uri": null,
        "set_req_args_state": null,
        "set_op_args": {
          "add_state": true
        },
        "id_token_hint": null
      }
    },
    "PostLogout",
    "AfterLogout"
  ]
}