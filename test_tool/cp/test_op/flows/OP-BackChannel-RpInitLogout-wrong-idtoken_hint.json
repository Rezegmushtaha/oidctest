{
  "group": "Back Channel Logout - RP Initiated",
  "usage": {
    "extra": true,
    "return_type": [
      "C",
      "CI",
      "CT",
      "CIT"
    ]
  },
  "desc": "Sent id_token_hint that is not based on a ID Token produced by the OP",
  "note": "This test should result in the OpenID Provider displaying an error message in your user agent. You should ignore the status of this test in the test tool, since it will be incomplete. You must submit a screen shot of the error shown as part of your certification application.",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "grant_types": [
            "authorization_code"
          ],
          "backchannel_logout_session_required": true
        },
        "set_backchannel_logout_uri": null
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "scope": [
            "openid",
            "offline_access"
          ],
          "prompt": "consent"
        }
      }
    },
    {
      "AccessToken": {
        "conditional_execution":{
          "return_type": ["CIT","CI","C","CT"]
        }
      }
    },
    "Note",
    {
      "EndSession": {
        "set_post_logout_redirect_uri": null,
        "set_req_args_state": null,
        "set_op_args": {
          "add_state": true
        },
        "create_idtoken_hint_other_issuer": {"sub": "SubjectID"},
        "set_expect_error": {
          "error": [
            "invalid_request"
          ],
          "stop": false
        }
      }
    }
  ]
}