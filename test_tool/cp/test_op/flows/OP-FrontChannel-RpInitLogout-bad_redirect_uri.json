{
  "group": "Front Channel Logout - RP Initiated",
  "usage": {
    "extra": true,
    "return_type": [
      "C",
      "CI",
      "CT",
      "CIT"
    ]
  },
  "desc": "Uses RP initiated logout to end a Session at the OP. The request has post_logout_redirect_uri and an id_token_hint",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "grant_types": [
            "authorization_code"
          ],
          "frontchannel_logout_session_required": true
        },
        "set_frontchannel_logout_uri": null
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "scope": [
            "openid",
            "offline_access"
          ],
          "prompt": "consent"
        }
      }
    },
    {
      "AccessToken": {
        "conditional_execution":{
          "return_type": ["CIT","CI","C","CT"]
        }
      }
    },
    {
      "EndSession": {
        "set_request_args": {
          "post_logout_redirect_uri": "https://example.com/post_logout"
        },
        "set_req_args_state": null,
        "set_op_args": {
          "add_state": true
        },
        "id_token_hint": null,
        "set_expect_error": {
          "error": [
            "bad_request"
          ],
          "stop": false
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "prompt": "none"
        }
      }
    }
  ]
}