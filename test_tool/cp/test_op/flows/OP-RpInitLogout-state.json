{
  "group": "RP Initiated Logout",
  "usage": {
    "extra": true,
    "return_type": [
      "C",
      "CI",
      "CT",
      "CIT"
    ]
  },
  "desc": "Uses RP initiated logout to end a Session at the OP. The request has state and post_logout_redirect_uri",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "grant_types": [
            "authorization_code"
          ]
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "scope": [
            "openid",
            "offline_access"
          ],
          "prompt": "consent"
        }
      }
    },
    {
      "EndSession": {
        "set_post_logout_redirect_uri": null,
        "set_op_args": {
          "add_state": true
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "prompt": "none"
        },
        "set_expect_error": {
          "error": [
            "login_required",
            "interaction_required"
          ],
          "stop": false
        }
      }
    }
  ]
}