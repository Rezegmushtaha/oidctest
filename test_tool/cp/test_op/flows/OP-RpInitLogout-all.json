{
  "group": "Session management - RP Initiated Logout",
  "usage": {
    "extra": true,
    "return_type": [
      "C",
      "CI",
      "CT",
      "CIT"
    ]
  },
  "desc": "Uses RP initiated logout to end a Session at the OP. The request has state, post_logout_redirect_uri and in_token_hint",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    {
      "Registration": {
        "set_request_args": {
          "grant_types": [
            "authorization_code"
          ]
        }
      }
    },
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "scope": [
            "openid",
            "offline_access"
          ],
          "prompt": "consent"
        }
      }
    },
    {
      "AccessToken": {
        "conditional_execution":{
          "return_type": ["CIT","CI","C","CT"]
        }
      }
    },
    {
      "LogoutPage": {
        "set_post_logout_redirect_uri": null,
        "set_state": null,
        "set_op_args": {
          "add_state": true,
          "prop": "id_token_hint"
        },
        "id_token_hint": null
      }
    },
    "PostLogout",
    "AfterLogout"
  ]
}